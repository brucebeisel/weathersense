<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import com.bdb.weather.display.day.*?>
<?import com.bdb.weather.display.windrose.*?>

<fx:root type="javafx.scene.layout.GridPane"   xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints percentWidth="50"/> 
        <ColumnConstraints percentWidth="25"/> 
        <ColumnConstraints percentWidth="25"/> 
    </columnConstraints>
    <rowConstraints>
        <RowConstraints percentHeight="33" />
        <RowConstraints percentHeight="34" />
        <RowConstraints percentHeight="33" />
    </rowConstraints>
    <DayTemperaturePane fx:id="temperaturePlot" GridPane.columnIndex="0" GridPane.rowIndex="0"/> 
    <DayHumidityPane fx:id="humidityPlot" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.columnSpan="2" /> 
    <DayRainPane fx:id="rainPlot" GridPane.columnIndex="0" GridPane.rowIndex="1"/> 
    <DayPressurePane fx:id="pressurePlot" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.columnSpan="2" /> 
    <DayWindPane fx:id="windPlot" GridPane.columnIndex="0" GridPane.rowIndex="2"/> 
    <WindDirPane fx:id="windDirectionPlot" GridPane.columnIndex="1" GridPane.rowIndex="2"/> 
    <WindRosePane fx:id="windRose" GridPane.columnIndex="2" GridPane.rowIndex="2"/> 
</fx:root>
